{
  "name": "VMRay",
  "license": "AGPL-V3",
  "author": "Nils Kuhnert, CERT-Bund",
  "url": "https://github.com/BSI-CERT-Bund/cortex-analyzers",
  "version": "3.0.0",
  "description": "VMRay Sandbox file analysis.",
  "dataTypeList": ["hash", "file"],
  "command": "VMRay/vmray.py",
  "baseConfig": "VMRay",
  "configurationItems": [
    {
      "name": "url",
      "description": "Define the URL of the service",
      "type": "string",
      "multi": false,
      "required": true
    },
    {
      "name": "key",
      "description": "Define the API key",
      "type": "string",
      "multi": false,
      "required": true
    },
    {
      "name": "certverify",
      "description": "Verify certificates",
      "type": "boolean",
      "multi": false,
      "required": true,
      "defaultValue": true
    },
    {
      "name": "certpath",
      "description": "Path to certificate file, in case of self-signed etc.",
      "type": "string",
      "multi": false,
      "required": false
    },
    {
      "name": "disablereanalyze",
      "description": "If set to true, samples won't get re-analyzed.",
      "type": "boolean",
      "multi": false,
      "required": false,
      "defaultValue": false
    }
  ]
}
