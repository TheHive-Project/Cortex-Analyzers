{
    "name": "Patrowl_GetReport",
    "version": "1.0.0",
    "author": "Nicolas Mattiocco",
    "url": "https://github.com/TheHive-Project/Cortex-Analyzers",
    "license": "AGPL-V3",
    "description": "Get the current Patrowl report for a fdqn, a domain or an IP address.",
    "dataTypeList": ["fqdn", "domain", "ip"],
    "baseConfig": "Patrowl",
    "config": {
      "service": "getreport"
    },
    "configurationItems": [
      {
        "name": "url",
        "description": "Define the PatrOwl url",
        "type": "string",
        "multi": false,
        "required": true
      },
      {
        "name": "api_key",
        "description": "Define the PatrOwl API Key",
        "type": "string",
        "multi": false,
        "required": true
      }
    ],
    "command": "Patrowl/patrowl.py"
}
